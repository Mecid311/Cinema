@model CinemaPro.Domain.Entity.Ticket
@{
    ViewData["Title"] = "Confirmation";
}


<div class="prs_title_main_sec_wrapper">
    <div class="prs_title_img_overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="prs_title_heading_wrapper">
                    <h2>Confirmation Screen</h2>
                    <ul>
                        <li>
                            <a asp-controller="movie" asp-action="index">Movie</a>
                        </li>
                        <li>&nbsp;&nbsp; >&nbsp;&nbsp; Confirmation Screen</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="st_bcc_main_main_wrapper float_left">
    <div class="st_bcc_main_wrapper">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="st_bcc_heading_wrapper float_left">
                        <i class="fa fa-check-circle"></i>
                        <h3> Ticket Number: @Model.Clientcode</h3>
                    </div>
                </div>
             
            </div>
        </div>
    </div>
</div>


